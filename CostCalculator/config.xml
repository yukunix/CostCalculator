<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.fortsolution.costcalculator" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>CostCalculator</name>
  <description>
        CostCalculator.
    </description>
  <author email="akmal@fortsolution.com" href="http://fortsolution.com">
      CostCalculator
    </author>
  <content src="index.html"/>
  <access origin="*"/>
